                    <label class="col-md-3 control-label small">OTH DET3</label>
                    <div class="col-md-4">
                        <input id="idOth3" ng-enter="ctrl.keyDidOth3" ng-disabled="ctrl.viewOnly" type="text" ng-model="ctrl.objStoreMerch.OTH_DET3 " /> </br>
                    </div>
                </div>
                <div class="form-group form-group-sm">
                    <label class="col-md-3 control-label small">OTH DET4</label>
                    <div class="col-md-4">
                        <input id="idOth4" ng-enter="ctrl.keyDidOth4" ng-disabled="ctrl.viewOnly" type="text" ng-model="ctrl.objStoreMerch.OTH_DET4" /> </br>
                    </div>
                </div>
